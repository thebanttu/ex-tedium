#!/usr/bin/env python3

import priv_init
import bantu.utils
import os, re, subprocess, sys, time
from bantu.media import bantu_media as bm

media_host = 'poseidon'

if __name__ == "__main__":
    if sys.argv[0].endswith("fm"):
        search = sys.argv[1:]
        bm.sync_downloaded_media(search, media_host)
        bm.move_items_from_the_basket()
